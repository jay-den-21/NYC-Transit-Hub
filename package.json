{
  "name": "nyc-transit-hub",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config frontend/vite.config.js",
    "build": "vite build --config frontend/vite.config.js",
    "preview": "vite preview --config frontend/vite.config.js",
    "start": "npm run server:dev",
    "server:dev": "tsx --env-file .env backend/src/server.ts",
    "server:build": "tsc -p backend/tsconfig.json",
    "server:start": "node backend/dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "gtfs-realtime-bindings": "^0.0.5",
    "mysql2": "^3.11.3",
    "leaflet": "^1.9.4",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.2.0"
  }
}
